<div>
  <h4 class="ui center aligned medium icon header">
    <i class="circular users blue icon"></i>
      Chatters
  </h4>
  <div class="ui <%= "centered" if @users.count == 1 %> three column stackable grid">
    <% @users.each do |user| %>
      <%= render "chatters", obj: user %>
    <% end %>
  </div>
  <%= render "pagination" if @users.count > 9 %>
</div>
